{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "CSV Compare",
  "version": "0.1.0",
  "identifier": "com.csv-compare-js.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "csv-compare-js",
        "width": 600,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/sparkles.icns"
    ]
  }
}